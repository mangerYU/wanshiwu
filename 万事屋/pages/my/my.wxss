/* pages/my/my.wxss */
page{
  height: 100%;
  width: 100%;
  background-color:#F7F7F7;
}
.head{
height: 300rpx;
display: flex;
flex-wrap: nowrap;
justify-content:center;
background-image: url('../../image/my/bg.jpg') no-repeat;
}
.header-imgs{
width: 570rpx;
display: flex;
flex-direction: row;
flex-wrap: nowrap;
justify-content: flex-start;
}
.prot{
width: 140rpx;
height: 140rpx;
margin-top:70rpx;
margin-right: 20rpx;
}
.prot image{
width: 140rpx;
height: 140rpx;
border-radius: 80rpx;
}
.grade{
width: 400rpx;
height: 140rpx;
margin-top:70rpx;
margin-right: 20rpx;
font-size: 30rpx;
display: flex;
flex-direction: column;
justify-content: center;
color: #FFFFFF;
}
.grade image{
  width: 54rpx;
  height: 30rpx;
  margin-left:10rpx;
}
.header-set{
width: 120rpx;
height: 120rpx;
margin-top: 70rpx;
}
.set-down{
height:120rpx;
width: 120rpx;
}
.face{
height: 260rpx;
margin-top: 21rpx;
background-color: #FFFFFF;
}

.face-order{
  font-size: 28rpx;
  color: #333333;
  height: 120rpx;
  display: flex;
}
.order{
   padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
width: 750rpx;
font-size: 28rpx;
}
.order-active{
  background-color: #ececec;
}

.img{
width: 50rpx;
height: 50rpx;
background-color: red;
margin-left: 25rpx;

}
.img image{
width: 50rpx;
height:50rpx;

}
.fa{
  display: flex;
flex-direction: column;
width: 110rpx;
height: 90rpx;
font-size:26rpx; 
text-align: center;
color: #333333;

}
.face-four{
height: 140rpx;
border-top: 1px #F7F7F7 solid;
display: flex;
flex-direction: row;
justify-content: space-around;
padding-top:30rpx;
}
.list{
  margin-top: 20rpx;
height: 450rpx;
background-color: greenyellow;
}
.lis{
  font-size: 26rpx;
  color: #333333;
  height: 90rpx;
  background-color: #FFFFFF;
  border-bottom: 1px solid #F7F7F7;
}
